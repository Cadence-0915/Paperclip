<h1>Sign-Up Here</h1>
  <%= form_for @user, :url => users_path, :html => { :multipart => true } do |f| %>
  <%=f.label :fname %>
  <%=f.text_field :fname %>
  <%=f.label :lname %>
  <%=f.text_field :lname %>
  <%=f.label :username %>
  <%=f.text_field :username %>
  <%=f.label :email %>
  <%=f.email_field :email %>
  <%=f.label :password %>
  <%=f.password_field :password %>
  <%= f.file_field :avatar %>
  <%=f.submit "Submit" %>
  
<% end %>